{
  "name": "GCR Renamer",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "./js/background.js"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://classroom.google.com/*"],
      "js": ["./js/rename.js"]
    }
  ],
  "action": {}
}